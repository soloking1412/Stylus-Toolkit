{
  "name": "stylus-toolkit",
  "version": "0.1.0",
  "description": "A comprehensive CLI development environment for Arbitrum Stylus smart contracts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "stylus-toolkit": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "echo \"Tests will be added in Milestone 2\" && exit 0",
    "prepare": "npm run build"
  },
  "keywords": [
    "arbitrum",
    "stylus",
    "cli",
    "smart-contracts",
    "rust",
    "solidity",
    "gas-profiling",
    "blockchain",
    "ethereum",
    "l2"
  ],
  "author": "Maheswaran Velmurugan <maheswar@example.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/soloking1412/stylus-toolkit.git"
  },
  "bugs": {
    "url": "https://github.com/soloking1412/stylus-toolkit/issues"
  },
  "homepage": "https://github.com/soloking1412/stylus-toolkit#readme",
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "chalk": "^4.1.2",
    "cli-table3": "^0.6.3",
    "commander": "^11.1.0",
    "conf": "^10.2.0",
    "dotenv": "^16.3.1",
    "ethers": "^6.9.0",
    "execa": "^5.1.1",
    "fast-csv": "^4.3.6",
    "fs-extra": "^11.2.0",
    "handlebars": "^4.7.8",
    "inquirer": "^8.2.6",
    "json2csv": "^6.0.0-alpha.2",
    "ora": "^5.4.1",
    "solc": "^0.8.33",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^8.2.10",
    "@types/jest": "^29.5.11",
    "@types/json2csv": "^5.0.7",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
